<div class="container">
 <div class="row">
  <div class="col m-3">
    <h4>いいね一覧</h4><br>
    <table class="table">
      <tbody>
        <% @favorites.each do |book| %>
        <tr>
          <th scope="low"><%= link_to book.title, public_book_path(book) %></th>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
 </div>
 <div class="d-flex pagenation justify-content-center mt-5 mb-3">
   <%= paginate @favorites %>
 </div>
</div>